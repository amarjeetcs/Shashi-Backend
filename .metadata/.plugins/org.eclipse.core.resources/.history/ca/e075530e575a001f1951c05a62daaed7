package javaprograms;

public class Hello {
	int j=10;
	static float _d=2.22f;
	
	public static void main(String args[]) {
		System.out.println("hello world");
		int i=2;
		System.out.println(i);
		Hello ob=new Hello();
		System.out.println(ob.j);
		System.out.println(Hello._d);
		System.out.println(_d);
		
	}

}
